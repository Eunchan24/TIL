    "n = int(input())\n",
    "\n",
    "arr = []\n",
    "\n",
    "for _ in range(n):\n",
    "    data = input().split(' ')\n",
    "    arr.append((int(data[0]), data[1]))\n",
    "\n",
    "arr = sorted(arr, key=lambda x:x[0])\n",
    "\n",
    "for i in arr:\n",
    "    print(i[0], i[1])"
